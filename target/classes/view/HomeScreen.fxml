<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="rootPane" prefHeight="800.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HomeScreenController">
   <top>
      <fx:include source="HeaderBar.fxml" />
   </top>
   <center>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
         <children>
            <StackPane id="center-stack-pane" fx:id="centerStackPane" alignment="CENTER_LEFT" prefHeight="235.0" prefWidth="1300.0">
               <children>
                  <ImageView fitHeight="250.0" fitWidth="1300.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/homepage banner.png" />
                     </image>
                  </ImageView>
                  <Button id="shopNowButton" fx:id="shopNowButton" mnemonicParsing="false" onAction="#clickShopNow" text="Shop Now" textFill="#404040">
                     <StackPane.margin>
                        <Insets left="20.0" top="25.0" />
                     </StackPane.margin>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <font>
                        <Font name="Arial Black" size="18.0" />
                     </font>
                     <padding>
                        <Insets bottom="10.0" left="50.0" right="50.0" top="10.0" />
                     </padding>
                  </Button>
               </children>
            </StackPane>
            <HBox alignment="CENTER" prefHeight="125.0" prefWidth="1100.0" spacing="50.0">
               <children>
                  <VBox id="categoryBox" fx:id="vboxHotFood" alignment="CENTER" onMouseClicked="#clickHotFoods" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" prefHeight="100.0" prefWidth="125.0">
                     <children>
                        <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/hot_food_icon_placeholder.png" />
                           </image>
                        </ImageView>
                        <Label text="Hot Foods" textFill="#404040">
                           <font>
                              <Font name="Arial Black" size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </VBox>
                  <VBox id="categoryBox" fx:id="vboxColdFood" alignment="CENTER" onMouseClicked="#clickColdFoods" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" prefHeight="200.0" prefWidth="125.0">
                     <children>
                        <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/cold_food_icon_palceholder.png" />
                           </image>
                        </ImageView>
                        <Label text="Cold Foods" textFill="#404040">
                           <font>
                              <Font name="Arial Black" size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </VBox>
                  <VBox id="categoryBox" fx:id="vboxSnacks" alignment="CENTER" onMouseClicked="#clickSnacks" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" prefHeight="200.0" prefWidth="125.0">
                     <children>
                        <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/snacks_icon_placeholder.png" />
                           </image>
                        </ImageView>
                        <Label text="Snacks" textFill="#404040">
                           <font>
                              <Font name="Arial Black" size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </VBox>
                  <VBox id="categoryBox" fx:id="vboxFruits" alignment="CENTER" onMouseClicked="#clickFruits" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" prefHeight="200.0" prefWidth="125.0">
                     <children>
                        <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/fruits_icon_placeholder.png" />
                           </image>
                        </ImageView>
                        <Label text="Fruits" textFill="#404040">
                           <font>
                              <Font name="Arial Black" size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </VBox>
                  <VBox id="categoryBox" fx:id="vboxDrinks" alignment="CENTER" onMouseClicked="#clickDrinks" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" prefHeight="200.0" prefWidth="125.0">
                     <children>
                        <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/drinks_icon_placeholder.png" />
                           </image>
                        </ImageView>
                        <Label text="Drinks" textFill="#404040">
                           <font>
                              <Font name="Arial Black" size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </VBox>
                  <VBox id="categoryBox" fx:id="vboxCoffee" alignment="CENTER" onMouseClicked="#clickCoffee" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" prefHeight="200.0" prefWidth="125.0">
                     <children>
                        <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/coffee_icon_placeholder.png" />
                           </image>
                        </ImageView>
                        <Label text="Coffee" textFill="#404040">
                           <font>
                              <Font name="Arial Black" size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets left="25.0" right="25.0" top="25.0" />
               </VBox.margin>
               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding>
            </HBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Popular favorites" textFill="#404040" underline="true">
                     <font>
                        <Font name="Montserrat SemiBold" size="24.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="25.0" />
                     </VBox.margin>
                  </Label>
                  <HBox fx:id="popularFavoritesHBox" alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <ImageView id="popFav" fx:id="popFavSandwich" fitHeight="200.0" fitWidth="200.0" onMouseClicked="#clickPopFavSandwich" pickOnBounds="true" preserveRatio="true">
                           <HBox.margin>
                              <Insets left="25.0" right="25.0" />
                           </HBox.margin>
                           <image>
                              <Image url="@../images/Chicken Sandwich.png" />
                           </image>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                        <ImageView id="popFav" fx:id="popFavIceCream" fitHeight="200.0" fitWidth="200.0" onMouseClicked="#clickPopFavIceCream" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/Ice Cream Bar.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="25.0" right="25.0" />
                           </HBox.margin>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                        <ImageView id="popFav" fx:id="popFavBurrito" fitHeight="200.0" fitWidth="200.0" onMouseClicked="#clickPopFavBurrito" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/Burrito.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="25.0" right="25.0" />
                           </HBox.margin>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                        <ImageView id="popFav" fx:id="popFavPizza" fitHeight="200.0" fitWidth="200.0" onMouseClicked="#clickPopFavPizza" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/Pizza.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="25.0" right="25.0" />
                           </HBox.margin>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                        <ImageView id="popFav" fx:id="popFavMuffin" fitHeight="200.0" fitWidth="200.0" onMouseClicked="#clickPopFavMuffin" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/BlueberryMuffin.png" />
                           </image>
                           <HBox.margin>
                              <Insets left="25.0" right="25.0" />
                           </HBox.margin>
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </ImageView>
                     </children></HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </center>
   <stylesheets>
      <URL value="@../css/home-screen-styles.css" />
   </stylesheets>
</BorderPane>
